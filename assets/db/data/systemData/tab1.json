{
  "name": "user",
  "tab": {
    "flutterType": "BottomNavigationBarItem",
    "icon": {
      "flutterType": "Icon",
      "src": "account_circle"
    },
    "label": "Home"
  },
  "content": {
    "flutterType": "Notify",
    "link": {
      "template": "Account.json"
    },
    "subscribeToRefresh": {
      "uuid": "Account.js"
    },
    "context": {
      "key": "Tab1",
      "data": {
        "template": {
          "flutterType": "Scaffold",
          "appBar": {
            "flutterType": "AppBar",
            "title": {
              "flutterType": "Text",
              "label": "Аккаунт"
            }
          }
        }
      }
    },
    "constructor": {
      "jsInvoke": "Account.js",
      "args": {
        "includeAll": true,
        "switch": "constructor"
      }
    },
    "onActive": {
      "jsInvoke": "Account.js",
      "args": {
        "includeAll": true,
        "switch": "onActive"
      }
    }
  }
}