{
  "flutterType": "Scaffold",
  "body": {
    "flutterType": "Padding",
    "padding": 10,
    "child": {
      "flutterType": "CustomScrollView",
      "includeBottomOffset": true,
      "onStateDataUpdate": true,
      "appBar": {
        "flutterType": "SliverAppBar",
        "title": {
          "flutterType": "Text",
          "label": "Тренировка ${container(inputData,data.label,WatchReady)}",
          "style": {
            "flutterType": "TextStyle",
            "fontSize": 17
          }
        }
      },
      "children": [
        {
          "flutterType": "SizedBox",
          "height": 20
        },
        {
          "flutterType": "Notify",
          "link": {
            "userData": "${pageArgument(userData)}"
          },
          "linkDefault": {
            "template": {
              "flutterType": "Column",
              "children": [
                {
                  "flutterType": "Text",
                  "label": "${state(sw.index1,0)}/${state(sw.size,0)}"
                },
                {
                  "flutterType": "SizedBox",
                  "height": 20
                },
                {
                  "flutterType": "LinearProgressIndicator",
                  "value": "${state(sw.prc1,0)}",
                  "backgroundColor": "schema:onBackground",
                  "color": "schema:secondary",
                  "semanticsLabel": "semanticsLabel",
                  "semanticsValue": "semanticsValue"
                }
              ]
            }
          },
          "templateArguments": [
            "link.userData"
          ]
        },
        {
          "flutterType": "SizedBox",
          "height": 30
        },
        {
          "flutterType": "Center",
          "child": {
            "flutterType": "Container",
            "width": 999,
            "height": 200,
            "child": {
              "flutterType": "SwipableStack",
              "roll": false,
              "viewFraction": 0,
              "key": "sw",
              "directions": [
                "left",
                "right"
              ],
              "overlay": {
                "flutterType": "Opacity",
                "opacity": "${state(sw.overlayOpacity)}",
                "child": {
                  "flutterType": "Row",
                  "mainAxisAlignment": "${state(sw.overlayDirection)|map(right,start,left,end)}",
                  "crossAxisAlignment": "start",
                  "children": [
                    {
                      "flutterType": "Container",
                      "width": 80,
                      "height": 44,
                      "margin": 10,
                      "decoration": {
                        "flutterType": "BoxDecoration",
                        "color": "schema:background",
                        "borderRadius": 10
                      },
                      "child": {
                        "flutterType": "Center",
                        "child": {
                          "flutterType": "Text",
                          "label": "${state(sw.overlayDirection)|map(right,Знаю,left,Не знаю)}"
                        }
                      }
                    }
                  ]
                }
              },
              "finish": {
                "flutterType": "Text",
                "label": "Finish"
              },
              "children": [
                {
                  "ChildrenExtension": "Iterator",
                  "dataType": "state",
                  "key": "card"
                }
              ],
              "onSwipeCompleted": {
                "jsInvoke": "Ex.js",
                "args": {
                  "includeAll": true,
                  "switch": "onSwipeCompleted"
                }
              }
            }
          }
        }
      ]
    }
  }
}