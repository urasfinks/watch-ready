{
  "flutterType": "Scaffold",
  "backgroundColor": "transparent",
  "body": {
    "flutterType": "SizedBox",
    "type": "expand",
    "child": {
      "flutterType": "Column",
      "mainAxisAlignment": "center",
      "children": [
        {
          "flutterType": "Text",
          "label": "Выучено ${pageArgs(message,)} материала"
        },
        {
          "flutterType": "SizedBox",
          "height": 30
        },
        {
          "flutterType": "Button",
          "backgroundColor": "schema:primaryContainer",
          "width": 200,
          "height": 44,
          "onPressed": {
            "sysInvoke": "NavigatorPop",
            "args": {
              "count": 2
            }
          },
          "child": {
            "flutterType": "Text",
            "label": "Назад",
            "textAlign": "center",
            "style": {
              "flutterType": "TextStyle",
              "fontSize": 15,
              "color": "schema:primary"
            }
          }
        },
        {
          "flutterType": "SizedBox",
          "height": 30
        },
        {
          "flutterType": "Button",
          "height": 44,
          "width": 200,
          "onPressed": {
            "sysInvoke": "NavigatorPop",
            "args": {
              "reloadParent": true
            }
          },
          "child": {
            "flutterType": "Text",
            "label": "Повторить",
            "textAlign": "center",
            "style": {
              "flutterType": "TextStyle",
              "fontSize": 15,
              "color": "white"
            }
          }
        }
      ]
    }
  }
}
