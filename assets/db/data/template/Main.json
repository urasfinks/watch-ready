{
  "flutterType": "Scaffold",
  "body": {
    "flutterType": "CustomScrollView",
    "onStateDataUpdate": true,
    "padding": "10,20,10,0",
    "appBar": {
      "flutterType": "SliverAppBar",
      "centerTitle": false,
      "title": {
        "flutterType": "Text",
        "label": "WatchReady",
        "textAlign": "left",
        "style": {
          "flutterType": "TextStyle",
          "fontSize": 17
        }
      },
      "actions": [
        {
          "flutterType": "Column",
          "mainAxisAlignment": "center",
          "children": [
            {
              "flutterType": "Button",
              "height": 34,
              "borderRadius": 20,
              "child": {
                "flutterType": "Text",
                "label": "Хочу больше сериалов",
                "textAlign": "center",
                "style": {
                  "flutterType": "TextStyle",
                  "fontSize": 14,
                  "color": "white"
                }
              },
              "onPressed": {
                "sysInvoke": "NavigatorPush",
                "args": {
                  "uuid": "SerialRequest.json"
                }
              }
            }
          ]
        },
        {
          "flutterType": "SizedBox",
          "width": 10
        }
      ]
    },
    "children": [
      {
        "ChildrenExtension": "Iterator",
        "dataType": "state",
        "key": "listSerial",
        "templateLabel": {
          "flutterType": "Padding",
          "padding": "0,0,0,20",
          "child": {
            "flutterType": "Text",
            "label": "${label}",
            "style": {
              "flutterType": "TextStyle",
              "fontSize": 17,
              "fontWeight": "bold",
              "color": "schema:secondary"
            }
          }
        },
        "templateSerialLine": {
          "flutterType": "Padding",
          "padding": "0,0,0,20",
          "child": {
            "flutterType": "Row",
            "children": [
              {
                "flutterType": "Expanded",
                "child": {
                  "flutterType": "ImageTitled",
                  "childKey": "${childKey1}",
                  "label": "${label1}",
                  "onTap": "${onTap1|jsonEncode()}",
                  "image": {
                    "flutterType": "ImageBase64",
                    "height": 130,
                    "width": "infinity",
                    "fit": "cover",
                    "src": "${src1}",
                    "controller": "${contextKey()}_1column"
                  },
                  "currentRenderTemplateList": [
                    "image.controller"
                  ],
                  "templateLockScreen": {
                    "flutterType": "Container",
                    "alignment": "topRight",
                    "color": "#000000|opacity(0.65)",
                    "child": {
                      "flutterType": "Padding",
                      "padding": "10",
                      "child": {
                        "flutterType": "Icon",
                        "src": "lock_outline",
                        "color": "white"
                      }
                    }
                  }
                }
              },
              {
                "flutterType": "SizedBox",
                "width": 20
              },
              {
                "flutterType": "Expanded",
                "child": {
                  "flutterType": "ImageTitled",
                  "childKey": "${childKey2}",
                  "label": "${label2}",
                  "onTap": "${onTap2|jsonEncode()}",
                  "image": {
                    "flutterType": "ImageBase64",
                    "height": 130,
                    "width": "infinity",
                    "fit": "cover",
                    "src": "${src2}",
                    "controller": "${contextKey()}_2column"
                  },
                  "currentRenderTemplateList": [
                    "image.controller"
                  ],
                  "templateLockScreen": {
                    "flutterType": "Container",
                    "alignment": "topRight",
                    "color": "#000000|opacity(0.65)",
                    "child": {
                      "flutterType": "Padding",
                      "padding": "10",
                      "child": {
                        "flutterType": "Icon",
                        "src": "lock_outline",
                        "color": "white"
                      }
                    }
                  }
                }
              }
            ]
          }
        }
      },
      {
        "flutterType": "SizedBox",
        "height": 20
      }
    ]
  }
}