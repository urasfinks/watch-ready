{
  "flutterType": "Scaffold",
  "body": {
    "flutterType": "CustomScrollView",
    "onStateDataUpdate": true,
    "padding": "10,20,10,0",
    "appBar": {
      "flutterType": "SliverAppBar",
      "centerTitle": false,
      "title": {
        "flutterType": "Text",
        "label": "WatchReady",
        "textAlign": "left",
        "style": {
          "flutterType": "TextStyle",
          "fontSize": 17
        }
      },
      "actions": [
        {
          "flutterType": "Column",
          "mainAxisAlignment": "center",
          "children": [
            {
              "flutterType": "Button",
              "height": 34,
              "borderRadius": 20,
              "child": {
                "flutterType": "Text",
                "label": "Хочу больше сериалов",
                "textAlign": "center",
                "style": {
                  "flutterType": "TextStyle",
                  "fontSize": 14,
                  "color": "white"
                }
              },
              "onPressed": {
                "sysInvoke": "NavigatorPush",
                "args": {
                  "uuid": "SerialRequest.json"
                }
              }
            }
          ]
        },
        {
          "flutterType": "SizedBox",
          "width": 10
        }
      ]
    },
    "children": [
      {
        "ChildrenExtension": "Iterator",
        "dataType": "state",
        "key": "listSerial",
        "startCard": {
          "flutterType": "Material",
          "color": "schema:onBackground",
          "shape": {
            "flutterType": "RoundedRectangleBorder",
            "borderRadius": "10"
          },
          "child": {
            "flutterType": "InkWell",
            "customBorder": {
              "flutterType": "RoundedRectangleBorder",
              "borderRadius": "10"
            },
            "onTap": "${onTap|jsonEncode()}",
            "child": {
              "flutterType": "Container",
              "padding": 15,
              "child": {
                "flutterType": "Row",
                "children": [
                  {
                    "flutterType": "Expanded",
                    "child": {
                      "flutterType": "Column",
                      "crossAxisAlignment": "start",
                      "children": [
                        {
                          "flutterType": "Text",
                          "style": {
                            "flutterType": "TextStyle",
                            "fontWeight": "bold"
                          },
                          "label": "${serialName} - ${lessName}"
                        },
                        {
                          "flutterType": "SizedBox",
                          "height": 5
                        },
                        {
                          "flutterType": "Text",
                          "style": {
                            "flutterType": "TextStyle",
                            "color": "schema:secondary"
                          },
                          "label": "${serialGroup}\r\n${lastScore}%"
                        },
                        {
                          "flutterType": "SizedBox",
                          "height": 5
                        },
                        {
                          "flutterType": "LinearProgressIndicator",
                          "valueIsPercent": true,
                          "value": "${lastScore}",
                          "backgroundColor": "schema:primaryContainer",
                          "color": "schema:onPrimary",
                          "semanticsLabel": "semanticsLabel",
                          "semanticsValue": "semanticsValue"
                        }
                      ]
                    }
                  },
                  {
                    "flutterType": "SizedBox",
                    "width": 30
                  },
                  {
                    "flutterType": "Container",
                    "width": 50,
                    "height": 50,
                    "decoration": {
                      "color": "schema:primaryContainer",
                      "flutterType": "BoxDecoration",
                      "borderRadius": 50
                    },
                    "child": {
                      "flutterType": "Icon",
                      "src": "play_arrow"
                    }
                  }
                ]
              }
            }
          }
        },
        "emptyStartCard": {
          "flutterType": "SizedBox"
        },
        "templateLabel": {
          "flutterType": "Padding",
          "padding": "5,20,0,20",
          "child": {
            "flutterType": "Text",
            "label": "${label}",
            "style": {
              "flutterType": "TextStyle",
              "fontSize": 17,
              "fontWeight": "bold",
              "color": "schema:secondary"
            }
          }
        },
        "templateSerialLine": {
          "flutterType": "Padding",
          "padding": "4,0,4,20",
          "child": {
            "flutterType": "Row",
            "children": [
              {
                "flutterType": "Expanded",
                "child": {
                  "flutterType": "ImageTitled",
                  "childKey": "${childKey1}",
                  "label": "${label1}",
                  "onTap": "${onTap1|jsonEncode()}",
                  "image": {
                    "flutterType": "ImageBase64",
                    "height": 130,
                    "width": "infinity",
                    "fit": "cover",
                    "src": "${src1}",
                    "controller": "${contextKey()}_1column"
                  },
                  "currentRenderTemplateList": [
                    "image.controller"
                  ],
                  "templateLockScreen": {
                    "flutterType": "Container",
                    "alignment": "topRight",
                    "color": "#000000|opacity(0.65)",
                    "child": {
                      "flutterType": "Padding",
                      "padding": "10",
                      "child": {
                        "flutterType": "Icon",
                        "src": "lock_outline",
                        "color": "white"
                      }
                    }
                  }
                }
              },
              {
                "flutterType": "SizedBox",
                "width": 20
              },
              {
                "flutterType": "Expanded",
                "child": {
                  "flutterType": "ImageTitled",
                  "childKey": "${childKey2}",
                  "label": "${label2}",
                  "onTap": "${onTap2|jsonEncode()}",
                  "image": {
                    "flutterType": "ImageBase64",
                    "height": 130,
                    "width": "infinity",
                    "fit": "cover",
                    "src": "${src2}",
                    "controller": "${contextKey()}_2column"
                  },
                  "currentRenderTemplateList": [
                    "image.controller"
                  ],
                  "templateLockScreen": {
                    "flutterType": "Container",
                    "alignment": "topRight",
                    "color": "#000000|opacity(0.65)",
                    "child": {
                      "flutterType": "Padding",
                      "padding": "10",
                      "child": {
                        "flutterType": "Icon",
                        "src": "lock_outline",
                        "color": "white"
                      }
                    }
                  }
                }
              }
            ]
          }
        }
      },
      {
        "flutterType": "SizedBox",
        "height": 20
      }
    ]
  }
}