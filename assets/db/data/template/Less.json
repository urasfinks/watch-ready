{
  "flutterType": "Scaffold",
  "body": {
    "flutterType": "CustomScrollView",
    "includeBottomOffset": true,
    "onStateDataUpdate": true,
    "padding": "10,20,10,0",
    "appBar": {
      "flutterType": "SliverAppBar",
      "title": {
        "flutterType": "Text",
        "label": "${contextByKey(inputData,data.label,WatchReady)}",
        "style": {
          "flutterType": "TextStyle",
          "fontSize": 17
        }
      }
    },
    "children": [
      {
        "flutterType": "Row",
        "children": [
          {
            "flutterType": "Expanded",
            "child": {
              "flutterType": "Button",
              "minimumSize": "9999,44",
              "backgroundColor": "blue",
              "onPressed": {
                "jsInvoke": "Less.js",
                "args": {
                  "includeAll": true,
                  "switch": "startExEng"
                }
              },
              "child": {
                "flutterType": "Text",
                "label": "Тренировать English",
                "textAlign": "center",
                "style": {
                  "flutterType": "TextStyle",
                  "fontSize": 14,
                  "color": "white"
                }
              }
            }
          },
          {
            "flutterType": "SizedBox",
            "width": 20
          },
          {
            "flutterType": "Expanded",
            "child": {
              "flutterType": "Button",
              "minimumSize": "9999,44",
              "backgroundColor": "blue",
              "onPressed": {
                "jsInvoke": "Less.js",
                "args": {
                  "includeAll": true,
                  "switch": "startExRus"
                }
              },
              "child": {
                "flutterType": "Text",
                "label": "Тренировать Русский",
                "textAlign": "center",
                "style": {
                  "flutterType": "TextStyle",
                  "fontSize": 14,
                  "color": "white"
                }
              }
            }
          }
        ]
      },
      {
        "flutterType": "SizedBox",
        "height": 20
      },
      {
        "ChildrenExtension": "Iterator",
        "dataType": "state",
        "key": "list",
        "theme": "ButtonGroup"
      }
    ]
  }
}